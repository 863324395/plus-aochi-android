<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--主题色-->
    <color name="themeColor">@color/important_for_theme</color>
    <!--背景色-->
    <color name="bgColor">@color/general_for_light_bg</color>
    <!--titilebar背景色   your app's branding color (for the app bar) -->
    <color name="colorPrimary">@color/themeColor</color>
    <!--titlebar 文字颜色-->
    <color name="textColorPrimary">@color/white</color>
    <!--状态栏背景色   darker variant of colorPrimary (for status bar, contextual app bars) -->
    <color name="colorPrimaryDark">@color/themeColor</color>
    <!--checkbox或者dialog按钮颜色    theme UI controls like checkboxes and text fields-->
    <color name="colorAccent">@color/themeColor</color>

    <!--common-->
    <color name="white">#ffffff</color>

    <!--toolbar-->
    <color name="toolbar_text_corlor_normal">#ffffff</color>
    <!--divider-->
    <color name="color_divider">#e1e1e1</color>

</resources>
